# Global snippets

# (c) holds no legal value ;)
snippet c)
	`&enc[:2] == "utf" ? "Â©" : "(c)"` Copyright `strftime("%Y")` ${1:`g:snips_author`}. All Rights Reserved.${2}
# Date -- 2009-09-15:
snippet date
	`strftime("%Y-%m-%d")`
# Pretty date -- September 15, 2009:
snippet pdate
	`strftime("%B %d, %Y")`
snippet name
	`g:snips_author`
snippet url
	<url:${1:http://localhost}>${2}
snippet google
	<url:http://google.com/search?q=${1}>${2}
# Usual dbext setup:
snippet dbext
	let g:dbext_default_profile_project = 'type=MYSQL:host=${1:localhost}:user=${2:root}:passwd=${3}:dbname=${4:test}'
	let g:dbext_default_profile = 'project'
