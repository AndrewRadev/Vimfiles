snippet -
	<% ${1} %>${2}
snippet =
	<%= ${1} %>${2}
snippet --
	<%- ${1} -%>${2}
snippet #
	<%# ${1} %>${2}
snippet each
	<% ${1:@collection}.each do |${2:item}| %>
	  ${3:<%# content %>}
	<% end %>${4}
snippet if
	<% if ${1:true} %>
	  ${3:<%# content %>}
	<% end %>${4}
snippet ife
	<% if ${1:true} %>
	  ${3:<%# content %>}
	<% else %>
	  ${4:<%# content %>}
	<% end %>
